{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**/**.csproj"
          ],
          "exclude": [
            ".git/**",
            ".vs/**",
            "Doc/**",
            "Isopoh.Cryptography.Test/**",
            "TestApp/**",
            "TestBlazor/**",
            "docfx_project/**",
            "docs/**",
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/isopoh"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}